		<!-- ===============    stellar core jquery     =============== -->
		<script type="text/javascript" src="/js/jquery.stellar.min.js"></script>

		<!-- ===============    smooth core jquery     =============== -->
		<script type="text/javascript" src="/js/smooth.js"></script>

		<!-- ===============    isotope core jquery     =============== -->
