		<!-- ===============    favicon link     =============== -->
		<link rel="icon" type="image/png" href="images/favicon.png" />

		<!-- ===============    Bootstrap core CSS     =============== -->
		<link rel="stylesheet" type="text/css" href="/styles/bootstrap.min.css" media="all" />

		<!-- ===============    font-awesome core CSS     =============== -->
		<link rel="stylesheet" type="text/css" href="/styles/font-awesome.min.css" media="all" />

		<!-- ===============    theme core CSS     =============== -->
		<link rel="stylesheet" type="text/css" href="/styles/effect1.css" media="all" />

